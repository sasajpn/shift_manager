<section class="content">
  <div class="box box-default">
    <%= form_for [:admins, @owner] do |f| %>
      <div class="box-header with-border">
        <h3 class="box-title">オーナー登録</h3>
      </div>
      <div class="box-body">
        <div class="row">
          <div class="col-md-6">
            <div class="form-group has-feedback">
              <%= f.label :email %>
              <%= f.email_field :email, autofocus: true, class: 'form-control' %>
            </div>
            <div class="form-group has-feedback">
              <%= f.label :password %>
              <%= f.password_field :password, autocomplete: "off", class: 'form-control' %>
            </div>
            <div class="form-group has-feedback">
              <%= f.label :password_confirmation %>
              <%= f.password_field :password_confirmation, autocomplete: "off", class: 'form-control' %>
            </div>
          </div>
          <div class="col-md-6">
            <div class="form-group">
              <%= f.label :last_name_kana %>
              <%= f.text_field :last_name_kana, class: 'form-control' %>
            </div>
            <div class="form-group">
              <%= f.label :first_name_kana %>
              <%= f.text_field :first_name_kana, class: 'form-control' %>
            </div>
            <div class="form-group">
              <%= f.label :last_name %>
              <%= f.text_field :last_name, class: 'form-control' %>
            </div>
            <div class="form-group">
              <%= f.label :first_name %>
              <%= f.text_field :first_name, class: 'form-control' %>
            </div>
          </div>
        </div>
      </div>
      <div class="box-footer">
        <%= f.submit '登録', class: 'btn btn-primary btn-block' %>
      </div>
    <% end %>
  </div>
</section>
