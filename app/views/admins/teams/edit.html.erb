<div class="row">
  <div class="col-md-6">
    <div class="box box-default">
      <%= bootstrap_form_for [:admins, @team] do |f| %>
        <div class="box-header with-border">
          <h3 class="box-title">チーム登録内容変更</h3>
        </div>
        <div class="box-body">
          <%= f.alert_message "以下のエラー内容を修正してください" %>
          <%= f.text_field :name %>
        </div>
        <div class="box-footer">
          <%= f.submit '更新', class: 'btn btn-primary btn-block' %>
        </div>
      <% end %>
    </div>
  </div>
</div>
